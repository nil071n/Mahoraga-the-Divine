local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")

-- Function to handle all animations after death
local function handleAnimationsAfterDeath()
    
    -- Function for Move 3
    local function onMove3AnimationPlayed(animationTrack)
        local animationId = "rbxassetid://10471336737"
        if animationTrack.Animation.AnimationId == animationId then
            local Humanoid = player.Character:WaitForChild("Humanoid")
            -- Stop currently playing animations
            for _, animTrack in pairs(Humanoid:GetPlayingAnimationTracks()) do
                animTrack:Stop()
            end

            -- Load and play new animation
            local AnimAnim = Instance.new("Animation")
            AnimAnim.AnimationId = "rbxassetid://17838619895"
            local Anim = Humanoid:LoadAnimation(AnimAnim)
            local startTime = 0.3

            Anim:Play()
            Anim:AdjustSpeed(0.8)
            Anim.TimePosition = startTime

            delay(1.8, function()
                Anim:Stop()
            end)
        end
    end

    -- Function for Move 4
    local function onMove4AnimationPlayed(animationTrack)
        local animationId = "rbxassetid://12510170988"
        if animationTrack.Animation.AnimationId == animationId then
            local Humanoid = player.Character:WaitForChild("Humanoid")
            -- Stop currently playing animations
            for _, animTrack in pairs(Humanoid:GetPlayingAnimationTracks()) do
                animTrack:Stop()
            end

            -- Load and play new animation
            local AnimAnim = Instance.new("Animation")
            AnimAnim.AnimationId = "rbxassetid://18897119503"
            local Anim = Humanoid:LoadAnimation(AnimAnim)
            local startTime = 0

            Anim:Play()
            Anim:AdjustSpeed(1)
            Anim.TimePosition = startTime
        end
    end

    -- Function for Wall Combo
    local function onWallComboAnimationPlayed(animationTrack)
        local animationId = "rbxassetid://15955393872"
        if animationTrack.Animation.AnimationId == animationId then
            local Humanoid = player.Character:WaitForChild("Humanoid")
            -- Stop currently playing animations
            for _, animTrack in pairs(Humanoid:GetPlayingAnimationTracks()) do
                animTrack:Stop()
            end

            -- Load and play new animation
            local AnimAnim = Instance.new("Animation")
            AnimAnim.AnimationId = "rbxassetid://15943915877"
            local Anim = Humanoid:LoadAnimation(AnimAnim)
            local startTime = 0.05

            Anim:Play()
            Anim:AdjustSpeed(0)
            Anim.TimePosition = startTime
            Anim:AdjustSpeed(1)
        end
    end

    -- Function for Ult Activation
    local function onUltActivationAnimationPlayed(animationTrack)
        local animationId = "rbxassetid://12447707844"
        if animationTrack.Animation.AnimationId == animationId then
            local Humanoid = player.Character:WaitForChild("Humanoid")
            -- Stop currently playing animations
            for _, animTrack in pairs(Humanoid:GetPlayingAnimationTracks()) do
                animTrack:Stop()
            end

            -- Load and play new animation
            local AnimAnim = Instance.new("Animation")
            AnimAnim.AnimationId = "rbxassetid://17862993552"
            local Anim = Humanoid:LoadAnimation(AnimAnim)
            local startTime = 0

            Anim:Play()
            Anim:AdjustSpeed(0)
            Anim.TimePosition = startTime
            Anim:AdjustSpeed(1)

            delay(3, function()
                Anim:Stop()
            end)
        end
    end

    -- Connect the animation events
    humanoid.AnimationPlayed:Connect(onMove3AnimationPlayed)
    humanoid.AnimationPlayed:Connect(onMove4AnimationPlayed)
    humanoid.AnimationPlayed:Connect(onWallComboAnimationPlayed)
    humanoid.AnimationPlayed:Connect(onUltActivationAnimationPlayed)
end

