local player = game.Players.LocalPlayer
local char = player.Character
local Humanoid = char.Humanoid
local hot = player.PlayerGui:WaitForChild("Hotbar")
local hotbar = hot:WaitForChild("Backpack"):WaitForChild("Hotbar")
-- 14516273501

local function cloneToolName(slot, text)
    local toolName = slot.ToolName
    if not slot:FindFirstChild("SkibidiGame") then
        toolName.Visible = false
        local clone = toolName:Clone()
        clone.Name = "SkibidiGame"
        clone.Parent = slot
        clone.Text = text
        clone.Visible = true
    elseif slot:FindFirstChild("SkibidiGame") then
        if slot:FindFirstChild("SkibidiGame").Text ~= text then
            toolName.Visible = false
            slot:FindFirstChild("SkibidiGame").Text = text
        end
    end
end
-- 10470104242
local magichealth = player.PlayerGui:WaitForChild("ScreenGui"):WaitForChild("MagicHealth")
local UltLabel = player.PlayerGui:WaitForChild("ScreenGui"):WaitForChild("MagicHealth"):WaitForChild("TextLabel")
UltLabel.Visible = false
local UltLabel = UltLabel:Clone()
UltLabel.Visible = true
UltLabel.Name = "SkibidiRizzlerGyattOhio"
UltLabel.Parent = magichealth
task.spawn(function()
    while true do
        UltLabel.Text = "Heavenly Restriction"
        for _, slot in ipairs(hotbar:GetChildren()) do
            if slot:FindFirstChild("Base") and slot.Base:FindFirstChild("ToolName") then
                local toolNameText = slot.Base.ToolName.Text
                if toolNameText == "Normal Punch" then
                    cloneToolName(slot.Base, "《Heavenly Strike》")
                elseif toolNameText == "Consecutive Punches" then
                    cloneToolName(slot.Base, "《Heavy Barrage》")
                elseif toolNameText == "Shove" then
                    cloneToolName(slot.Base, "《Close Combat》")
                elseif toolNameText == "Uppercut" then
                    cloneToolName(slot.Base, "《Ground Break》")
                elseif toolNameText == "《Adapt》" then
                    cloneToolName(slot.Base, "Bang")
                elseif toolNameText == "《Table Flip》" then
                    cloneToolName(slot.Base, "World Cutting Slash》")
                elseif toolNameText == "Serious Punch" then
                    cloneToolName(slot.Base, "《Divine Strength》")
                elseif toolNameText == "Omni Directional Punch" then
                    cloneToolName(slot.Base, "《Earth Shaking Downslam》")
                end
            end
        end
        task.wait(1)
    end
end)



local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")

-- Function to handle animation playing logic for both moves
local function handleAnimationsAfterDeath()
    
    -- Function to handle Move 1
    local function onMove1AnimationPlayed(animationTrack)
        local animationId = "rbxassetid://10468665991"
        if animationTrack.Animation.AnimationId == animationId then
            local Humanoid = player.Character:WaitForChild("Humanoid")
            -- Stop any currently playing animations
            for _, animTrack in pairs(Humanoid:GetPlayingAnimationTracks()) do
                animTrack:Stop()
            end

            -- Load and play new animation
            local AnimAnim = Instance.new("Animation")
            AnimAnim.AnimationId = "rbxassetid://18896127525"
            local Anim = Humanoid:LoadAnimation(AnimAnim)
            local startTime = 0

            Anim:Play()
            Anim:AdjustSpeed(0.8)
            Anim.TimePosition = startTime

            delay(1.2, function()
                Anim:Stop()
            end)
        end
    end

    -- Function to handle Move 2
    local function onMove2AnimationPlayed(animationTrack)
        local animationId = "rbxassetid://10466974800"
        if animationTrack.Animation.AnimationId == animationId then
            local Humanoid = player.Character:WaitForChild("Humanoid")
            -- Stop any currently playing animations
            for _, animTrack in pairs(Humanoid:GetPlayingAnimationTracks()) do
                animTrack:Stop()
            end

            -- Load and play new animation
            local AnimAnim = Instance.new("Animation")
            AnimAnim.AnimationId = "rbxassetid://15090734317"
            local Anim = Humanoid:LoadAnimation(AnimAnim)
            local startTime = 0

            Anim:Play()
            Anim:AdjustSpeed(1)
            Anim.TimePosition = startTime
            Anim:AdjustSpeed(0.5)

            delay(1.8, function()
                Anim:Stop()
            end)
        end
    end

    -- Connect the animation events
    humanoid.AnimationPlayed:Connect(onMove1AnimationPlayed)
    humanoid.AnimationPlayed:Connect(onMove2AnimationPlayed)
end

-- Detect when the player dies and respawns
local function onPlayerDied()
    -- Wait for character to respawn
    player.CharacterAdded:Wait()
    humanoid = player.Character:WaitForChild("Humanoid")
    handleAnimationsAfterDeath()
end

-- Connect the death event to trigger the animations after respawn
humanoid.Died:Connect(onPlayerDied)

-- Initial setup when the game starts
handleAnimationsAfterDeath()






